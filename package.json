{
  "name": "voice-call",
  "version": "0.0.1",
  "description": "语音对讲",
  "scripts": {
    "deps": "npm run clean && cnpm install && lerna bootstrap",
    "clean": "cnpm install lerna@6 && lerna clean --yes && rm -rf ./node_modules",
    "build": "lerna run build --scope=voice-call",
    "build:cdn": "node ./build-cdn.mjs && cp demo.html dist/demo.html",
    "build:demo": "lerna run build --scope=demo",
    "demo": "lerna run dev --scope=demo",
    "ws": "lerna run ws --scope=demo",
    "zip": "node archive.mjs",
    "release": "npm run build && npm publish ./packages/voice-call",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.mjs --cache --fix --ignore-path .eslintignore ./",
    "test": "lerna run test",
    "add": "lerna add",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "repository": "https://github.com/fengxinming/voice-call.git",
  "author": "Jesse Feng <fxm0016@126.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-react": "^7.22.15",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "eslint": "^8.48.0",
    "eslint-config-fe": "^2.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.5.0"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@vitejs/plugin-react": "^4.1.1",
    "archiver": "^6.0.1",
    "fs-extra": "^11.1.1",
    "lerna": "^6.6.2",
    "sass": "^1.69.5",
    "terser": "^5.24.0",
    "vite": "^4.5.0",
    "vite-plugin-external": "^4.3.0"
  }
}
